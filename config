# ^^^ IMP - do not add trailing comma

# Combine all
# Core joomla comp, mod, plugins
PHPCS_IGNORE=${IGNORE_CORE_JOOMLA_COMPONENTS}${IGNORE_CORE_JOOMLA_MODULES}${IGNORE_CORE_JOOMLA_PLUGINS}
# Core joomla other + lib + layout files
PHPCS_IGNORE=${PHPCS_IGNORE}${IGNORE_JOOMLA_LIBRARIES}${IGNORE_CORE_JOOMLA_OTHER_FILES}${IGNORE_LAYOUT_FILES}

# Media folders
PHPCS_IGNORE=${PHPCS_IGNORE}${IGNORE_MEDIA_FOLDERS_ALL}

# 3rd party joomla extensions
PHPCS_IGNORE=${PHPCS_IGNORE}${IGNORE_3PD_JOOMLA_COMPONENTS}${IGNORE_3PD_JOOMLA_MODULES}${IGNORE_3PD_JOOMLA_PLUGINS}
# 3rd party joomla libs & layouts
PHPCS_IGNORE=${PHPCS_IGNORE}${IGNORE_3PD_JOOMLA_LIBRARIES}${IGNORE_3PD_JOOMLA_LAYOUTS}

# 3rd party libraries / classes
PHPCS_IGNORE=${PHPCS_IGNORE}${IGNORE_3PD_FRAMEWORKS}${IGNORE_3PD_LIBRARIES}${IGNORE_3PD_CLASSES}

# Techjoomla extensions
PHPCS_IGNORE=${PHPCS_IGNORE}${IGNORE_TECHJOOMLA_COMPONENTS}${IGNORE_TECHJOOMLA_PLUGINS}${IGNORE_TECHJOOMLA_LAYOUTS}${IGNORE_TECHJOOMLA_SCRIPTS}${IGNORE_TECJOOMLA_SPECIAL_FILES}

# Service projects
PHPCS_IGNORE=${PHPCS_IGNORE}${IGNORE_SERVICE_PROJECTS_SPECIAL_FILES}

# Service project components
PHPCS_IGNORE=${PHPCS_IGNORE}${IGNORE_SERVICE_PROJECTS_COMPONENTS}${IGNORE_SERVICE_PROJECTS_MODULES}${IGNORE_SERVICE_PROJECTS_PLUGINS}${IGNORE_TEST_CASES_SPECIAL_FILES_FOLDERS}

# Laravel Ignore
PHPCS_IGNORE=${PHPCS_IGNORE}${IGNORE_LARAVEL_LIBRARIES}

# Remove \n
PHPCS_IGNORE=$(echo $PHPCS_IGNORE|tr -d '\n')
# Remove spaces
PHPCS_IGNORE=$(echo "$PHPCS_IGNORE" | sed 's/ //g')

